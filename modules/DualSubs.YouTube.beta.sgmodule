[Rule]
URL-REGEX,^https?:\/\/([\w-]+)\.googlevideo\.com\/initplayback(\?.*oad=.+)$,REJECT-TINYGIF

[Script]
DualSubs.YouTube.Player.request.json = type=http-request, pattern=^https?:\/\/(www|m|tv)\.youtube\.com\/youtubei\/v1\/player, requires-body=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/YouTube/beta/js/DualSubs.YouTube.request.beta.js, argument=
DualSubs.YouTube.Player.response.json = type=http-response, pattern=^https?:\/\/(www|m|tv)\.youtube\.com\/youtubei\/v1\/player, requires-body=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/YouTube/beta/js/DualSubs.YouTube.Player.response.beta.js, argument=
DualSubs.YouTube.Player.request.proto = type=http-request, pattern=^https?:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/player, requires-body=1, binary-body-mode=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/YouTube/beta/js/DualSubs.YouTube.request.beta.js, argument=
DualSubs.YouTube.Player.response.proto = type=http-response, pattern=^https?:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/player, requires-body=1, binary-body-mode=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/YouTube/beta/js/DualSubs.YouTube.Player.response.beta.js, argument=
DualSubs.YouTube.TimedText.request = type=http-request, pattern=^https?:\/\/(www|m)\.youtube\.com\/api\/timedtext, requires-body=0, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/YouTube/beta/js/DualSubs.YouTube.TimedText.request.beta.js, argument=
DualSubs.YouTube.TimedText.Composite.response = type=http-response, pattern=^https?:\/\/(www|m)\.youtube\.com\/api\/timedtext\?(.*)subtype=(Official|External), requires-body=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/Universal/beta/js/DualSubs.Subtitles.Composite.response.beta.js, argument=
DualSubs.YouTube.TimedText.Translate.response = type=http-response, pattern=^https?:\/\/(www|m)\.youtube\.com\/api\/timedtext\?(.*)subtype=Translate, requires-body=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/Universal/beta/js/DualSubs.Subtitles.Translate.response.beta.js, argument=

DualSubs.YouTubeMusic.Browse.request.json = type=http-request, pattern=^https?:\/\/music\.youtube\.com\/youtubei\/v1\/browse, requires-body=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/YouTube/beta/js/DualSubs.YouTube.request.beta.js, argument=
DualSubs.YouTubeMusic.Browse.request.proto = type=http-request, pattern=^https?:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/browse, requires-body=1, binary-body-mode=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/YouTube/beta/js/DualSubs.YouTube.request.beta.js, argument=
DualSubs.YouTubeMusic.Lyrics.Translate.response = type=http-response, pattern=^https?:\/\/music\.youtube\.com\/youtubei\/v1\/browse\?(.*)subtype=Translate, requires-body=1, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/Universal/beta/js/DualSubs.Subtitles.Translate.response.beta.js, argument=

