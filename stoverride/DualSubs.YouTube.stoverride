name: 🍿️ DualSubs for ▶️ YouTube
desc: (V0.5) YouTube字幕增强及双语模块
openUrl: http://boxjs.com/#/app/DualSubs.YouTube
author: VirgilClyne
homepage: https://github.com/DualSubs
manual: https://github.com/DualSubs/DualSubs-YouTube/wiki/🍿-DualSubs-for-▶-YouTube
icon: https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/YouTube.png

http:
  mitm:
    - "www.youtube.com"
    - "m.youtube.com"
    - "tv.youtube.com"
    - "youtubei.googleapis.com"
  script:
    - match: ^https?:\/\/(www|m|tv)\.youtube\.com\/youtubei\/v1\/player(\?.*)$
      name: DualSubs.YouTube.Player
      type: response
      require-body: true
      timeout: 10
      argument:
    - match: ^https?:\/\/(www|m)\.youtube\.com\/api\/timedtext(\?.*)$
      name: DualSubs.YouTube.TimedText.request
      type: request
      require-body: false
      timeout: 10
      argument:
    - match: ^https?:\/\/(www|m)\.youtube\.com\/api\/timedtext(\?.*)$
      name: DualSubs.YouTube.TimedText.response
      type: response
      require-body: true
      timeout: 10
      argument:

script-providers:
  DualSubs.YouTube.Player:
    url: https://raw.githubusercontent.com/DualSubs/DualSubs-YouTube/v0.5.16/js/DualSubs.YouTube.Player.response.js
    interval: 86400
  DualSubs.YouTube.TimedText.request:
    url: https://raw.githubusercontent.com/DualSubs/DualSubs-YouTube/v0.5.16/js/DualSubs.YouTube.TimedText.request.js
    interval: 86400
  DualSubs.YouTube.TimedText.response:
    url: https://raw.githubusercontent.com/DualSubs/DualSubs-YouTube/v0.5.16/js/DualSubs.YouTube.TimedText.response.js
    interval: 86400
